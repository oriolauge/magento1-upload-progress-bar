<config>
    <modules>
        <Oag_UploadProgressBar>
            <version>1.1.2</version>
        </Oag_UploadProgressBar>
    </modules>
    <global>
        <blocks>
            <oag_uploadprogressbar>
                <class>Oag_UploadProgressBar_Block</class>
            </oag_uploadprogressbar>
        </blocks>
    </global>
    <frontend>
        <routers>
            <redsys>
                <use>standard</use>
                <args>
                    <module>Oag_UploadProgressBar</module>
                    <frontName>uploadprogressbar</frontName>
                </args>
            </redsys>
        </routers>
        <layout>
            <updates>
                <oag_uploadprogressbar>
                    <file>oag_uploadprogressbar.xml</file>
                </oag_uploadprogressbar>
            </updates>
        </layout>
        <translate>
            <modules>
                <Oag_UploadProgressBar>
                    <files>
                        <default>Oag_UploadProgressBar.csv</default>
                    </files>
                </Oag_UploadProgressBar>
            </modules>
        </translate>
    </frontend>
</config>